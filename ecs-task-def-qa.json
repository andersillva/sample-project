{
    "family": "sample-project-task",
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::<sua-conta>:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sample-project",
            "image": "andersillva/sample-project:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
